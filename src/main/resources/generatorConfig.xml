<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<properties
		url="file:/D:/work/git/sam_svr/src/main/resources/config/jdbc.properties" />
	<context id="application">

		<commentGenerator>
			<!-- <property name="suppressAllComments" value="true" /> -->
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="${jdbc.driverClass}"
			connectionURL="${jdbc.url}" userId="${jdbc.user}" password="${jdbc.password}">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.skyworld.beans.auto.entitybeans"
			targetProject="${generate.src.dir}">
			<property name="rootClass" value="com.skyworld.beans.auto.BaseEntity" />
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mysql.auto"
			targetProject="${generate.config.dir}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.skyworld.beans.auto.mapper"
			targetProject="${generate.src.dir}">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		
		<table tableName="tb_frm_dict_info" domainObjectName="TbFrmDictInfo">
			<property name="useActualColumnNames" value="true" />
			<generatedKey column="dict_id" sqlStatement="MySql"	identity="true" />
		</table>
		
	</context>

</generatorConfiguration>